{
  "REST-SPEC Package.json": {
    "prefix": "rest-package",
    "body": [
      "{",
      "  \"name\": \"${1:project-name}\",",
      "  \"version\": \"1.0.0\",",
      "  \"description\": \"${2:Project description}\",",
      "  \"main\": \"src/app.js\",",
      "  \"scripts\": {",
      "    \"start\": \"node src/app.js\",",
      "    \"dev\": \"nodemon src/app.js\",",
      "    \"test\": \"jest\",",
      "    \"test:watch\": \"jest --watch\",",
      "    \"test:coverage\": \"jest --coverage\",",
      "    \"lint\": \"eslint src/\",",
      "    \"lint:fix\": \"eslint src/ --fix\",",
      "    \"format\": \"prettier --write src/\",",
      "    \"migrate\": \"sequelize-cli db:migrate\",",
      "    \"migrate:undo\": \"sequelize-cli db:migrate:undo\",",
      "    \"seed\": \"sequelize-cli db:seed:all\",",
      "    \"docs\": \"swagger-jsdoc -d swaggerDef.js -o swagger.json src/routes/*.js\"",
      "  },",
      "  \"keywords\": [",
      "    \"rest\",",
      "    \"api\",",
      "    \"express\",",
      "    \"nodejs\"",
      "  ],",
      "  \"author\": \"${3:Author Name}\",",
      "  \"license\": \"MIT\",",
      "  \"dependencies\": {",
      "    \"express\": \"^4.18.2\",",
      "    \"helmet\": \"^6.0.1\",",
      "    \"cors\": \"^2.8.5\",",
      "    \"morgan\": \"^1.10.0\",",
      "    \"winston\": \"^3.8.2\",",
      "    \"joi\": \"^17.7.0\",",
      "    \"bcryptjs\": \"^2.4.3\",",
      "    \"jsonwebtoken\": \"^9.0.0\",",
      "    \"sequelize\": \"^6.28.0\",",
      "    \"mysql2\": \"^3.0.1\",",
      "    \"dotenv\": \"^16.0.3\",",
      "    \"express-rate-limit\": \"^6.7.0\",",
      "    \"swagger-jsdoc\": \"^6.2.8\",",
      "    \"swagger-ui-express\": \"^4.6.0\"",
      "  },",
      "  \"devDependencies\": {",
      "    \"jest\": \"^29.3.1\",",
      "    \"supertest\": \"^6.3.3\",",
      "    \"nodemon\": \"^2.0.20\",",
      "    \"eslint\": \"^8.31.0\",",
      "    \"eslint-config-airbnb-base\": \"^15.0.0\",",
      "    \"eslint-plugin-import\": \"^2.27.5\",",
      "    \"prettier\": \"^2.8.1\",",
      "    \"sequelize-cli\": \"^6.5.2\"",
      "  },",
      "  \"engines\": {",
      "    \"node\": \">=18.0.0\"",
      "  },",
      "  \"repository\": {",
      "    \"type\": \"git\",",
      "    \"url\": \"${4:https://github.com/username/repo.git}\"",
      "  }",
      "}"
    ],
    "description": "Complete package.json for REST-SPEC projects"
  },
  "ESLint Config": {
    "prefix": "eslint-config",
    "body": [
      "{",
      "  \"env\": {",
      "    \"node\": true,",
      "    \"es2021\": true,",
      "    \"jest\": true",
      "  },",
      "  \"extends\": [",
      "    \"eslint:recommended\",",
      "    \"airbnb-base\"",
      "  ],",
      "  \"parserOptions\": {",
      "    \"ecmaVersion\": \"latest\",",
      "    \"sourceType\": \"module\"",
      "  },",
      "  \"rules\": {",
      "    \"no-console\": \"warn\",",
      "    \"no-unused-vars\": \"error\",",
      "    \"prefer-const\": \"error\",",
      "    \"no-var\": \"error\",",
      "    \"object-shorthand\": \"error\",",
      "    \"prefer-arrow-callback\": \"error\",",
      "    \"max-len\": [\"error\", { \"code\": 100 }],",
      "    \"indent\": [\"error\", 2],",
      "    \"quotes\": [\"error\", \"single\"],",
      "    \"semi\": [\"error\", \"always\"],",
      "    \"comma-dangle\": [\"error\", \"never\"],",
      "    \"no-trailing-spaces\": \"error\",",
      "    \"eol-last\": \"error\"",
      "  }",
      "}"
    ],
    "description": "ESLint configuration for REST-SPEC projects"
  },
  "Jest Config": {
    "prefix": "jest-config",
    "body": [
      "{",
      "  \"testEnvironment\": \"node\",",
      "  \"setupFilesAfterEnv\": [\"<rootDir>/tests/setup.js\"],",
      "  \"testMatch\": [",
      "    \"<rootDir>/tests/**/*.test.js\",",
      "    \"<rootDir>/tests/**/*.spec.js\"",
      "  ],",
      "  \"collectCoverageFrom\": [",
      "    \"src/**/*.js\",",
      "    \"!src/app.js\",",
      "    \"!src/config/**\",",
      "    \"!**/node_modules/**\"",
      "  ],",
      "  \"coverageDirectory\": \"coverage\",",
      "  \"coverageReporters\": [",
      "    \"text\",",
      "    \"lcov\",",
      "    \"html\"",
      "  ],",
      "  \"coverageThreshold\": {",
      "    \"global\": {",
      "      \"branches\": 70,",
      "      \"functions\": 70,",
      "      \"lines\": 70,",
      "      \"statements\": 70",
      "    }",
      "  },",
      "  \"testTimeout\": 10000,",
      "  \"verbose\": true,",
      "  \"bail\": false,",
      "  \"maxWorkers\": \"50%\"",
      "}"
    ],
    "description": "Jest configuration for REST-SPEC projects"
  },
  "Docker Compose": {
    "prefix": "docker-compose",
    "body": [
      "{",
      "  \"version\": \"3.8\",",
      "  \"services\": {",
      "    \"app\": {",
      "      \"build\": \".\",",
      "      \"ports\": [",
      "        \"3000:3000\"",
      "      ],",
      "      \"environment\": [",
      "        \"NODE_ENV=development\",",
      "        \"DB_HOST=db\",",
      "        \"DB_PORT=3306\",",
      "        \"DB_NAME=${1:database_name}\",",
      "        \"DB_USER=${2:db_user}\",",
      "        \"DB_PASS=${3:db_password}\"",
      "      ],",
      "      \"depends_on\": [",
      "        \"db\",",
      "        \"redis\"",
      "      ],",
      "      \"volumes\": [",
      "        \"./src:/app/src\",",
      "        \"./tests:/app/tests\"",
      "      ]",
      "    },",
      "    \"db\": {",
      "      \"image\": \"mysql:8.0\",",
      "      \"environment\": [",
      "        \"MYSQL_DATABASE=${1:database_name}\",",
      "        \"MYSQL_USER=${2:db_user}\",",
      "        \"MYSQL_PASSWORD=${3:db_password}\",",
      "        \"MYSQL_ROOT_PASSWORD=${4:root_password}\"",
      "      ],",
      "      \"ports\": [",
      "        \"3306:3306\"",
      "      ],",
      "      \"volumes\": [",
      "        \"mysql_data:/var/lib/mysql\"",
      "      ]",
      "    },",
      "    \"redis\": {",
      "      \"image\": \"redis:7-alpine\",",
      "      \"ports\": [",
      "        \"6379:6379\"",
      "      ]",
      "    }",
      "  },",
      "  \"volumes\": {",
      "    \"mysql_data\": {}",
      "  }",
      "}"
    ],
    "description": "Docker Compose configuration for REST-SPEC projects"
  },
  "Swagger Definition": {
    "prefix": "swagger-def",
    "body": [
      "{",
      "  \"openapi\": \"3.0.0\",",
      "  \"info\": {",
      "    \"title\": \"${1:API Title}\",",
      "    \"version\": \"1.0.0\",",
      "    \"description\": \"${2:API Description}\",",
      "    \"contact\": {",
      "      \"name\": \"${3:Contact Name}\",",
      "      \"email\": \"${4:contact@example.com}\"",
      "    }",
      "  },",
      "  \"servers\": [",
      "    {",
      "      \"url\": \"http://localhost:3000/api\",",
      "      \"description\": \"Development server\"",
      "    },",
      "    {",
      "      \"url\": \"https://api.example.com\",",
      "      \"description\": \"Production server\"",
      "    }",
      "  ],",
      "  \"components\": {",
      "    \"securitySchemes\": {",
      "      \"bearerAuth\": {",
      "        \"type\": \"http\",",
      "        \"scheme\": \"bearer\",",
      "        \"bearerFormat\": \"JWT\"",
      "      }",
      "    },",
      "    \"schemas\": {",
      "      \"Error\": {",
      "        \"type\": \"object\",",
      "        \"properties\": {",
      "          \"success\": {",
      "            \"type\": \"boolean\",",
      "            \"example\": false",
      "          },",
      "          \"message\": {",
      "            \"type\": \"string\"",
      "          },",
      "          \"code\": {",
      "            \"type\": \"string\"",
      "          }",
      "        }",
      "      },",
      "      \"Success\": {",
      "        \"type\": \"object\",",
      "        \"properties\": {",
      "          \"success\": {",
      "            \"type\": \"boolean\",",
      "            \"example\": true",
      "          },",
      "          \"data\": {",
      "            \"type\": \"object\"",
      "          }",
      "        }",
      "      }",
      "    }",
      "  },",
      "  \"security\": [",
      "    {",
      "      \"bearerAuth\": []",
      "    }",
      "  ]",
      "}"
    ],
    "description": "Swagger/OpenAPI 3.0 definition for REST-SPEC APIs"
  }
}
